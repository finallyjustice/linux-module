# Set default values for all following accounts.
defaults
logfile ~/.msmtp.log
 
# gmail
account gmail
protocol smtp
host smtp.gmail.com
from FirstName LastName
user user@gmail.com
port 587
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
syslog LOG_MAIL 
 
# Set a default account
account default : gmail
